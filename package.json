{
  "name": "eternal-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stores": "jest stores/",
    "test:hooks": "jest hooks/",
    "test:accessibility": "jest --testNamePattern='Accessibility'",
    "test:performance": "jest --testNamePattern='Performance'",
    "test:integration": "jest --testNamePattern='Integration'",
    "build:analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-results",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run type-check && npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
